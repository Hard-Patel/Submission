{
  "name": "Energizer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "podinstall": "cd ios && pod install && cd ..",
    "postinstall": "yarn run podinstall",
    "postuninstall": "patch-package; npx jetify; yarn run podinstall;",
    "prettier": "prettier --write \"./src/**/*.{js,tsx,md,yml}\"",
    "format": "yarn run prettier",
    "format-check": "prettier --list-different \"./**/*.{js,tsx,md,yml}\"",
    "clean": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && yarn install && say Project has been cleaned successfully",
    "clean-android": "cd android && ./gradlew clean && cd ../ && yarn add jetifier && npx jetify",
    "generate-icons": "./scripts/icon-generator/icon-generator.sh;",
    "dev-release": "concurrently npm:release:dev:*",
    "release:dev:android": "bundle exec fastlane android ",
    "release:dev:ios": "bundle exec fastlane ios ",
    "e2e:build-ios": "detox build --configuration ios",
    "e2e:test-ios": "detox test --configuration ios",
    "e2e:build-android": "detox build --configuration android",
    "e2e:test-android": "detox test --configuration android",
    "run:ios": "npx react-native run-ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "bundle-android": " react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
    "@pavelgric/react-native-theme-provider": "^0.2.0",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-community/slider": "^4.5.0",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-navigation/bottom-tabs": "^6.5.18",
    "@react-navigation/material-top-tabs": "^6.6.11",
    "@react-navigation/native": "^6.1.15",
    "@react-navigation/native-stack": "^6.9.24",
    "@reduxjs/toolkit": "^2.2.1",
    "apisauce": "^3.0.1",
    "axios": "^1.6.7",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "formik": "^2.4.5",
    "i18next": "^23.10.1",
    "metro-react-native-babel-preset": "^0.77.0",
    "moment": "^2.30.1",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.6",
    "react-native-actionsheet": "^2.4.2",
    "react-native-background-timer": "^2.4.1",
    "react-native-config": "^1.5.1",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mask-input": "^1.2.3",
    "react-native-mmkv": "^2.12.1",
    "react-native-offline": "^6.0.2",
    "react-native-otp-textinput": "^1.1.5",
    "react-native-pager-view": "^6.2.3",
    "react-native-reanimated": "^3.8.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-toast-message": "^2.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
